<?xml version="1.0"?>
<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">

    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />

    <rosparam file="$(find racebot_gazebo)/config/xt_teb/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find racebot_gazebo)/config/xt_teb/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find racebot_gazebo)/config/xt_teb/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find racebot_gazebo)/config/xt_teb/global_costmap_params.yaml" command="load" />

    <rosparam file="$(find racebot_gazebo)/config/xt_teb/move_base_params.yaml" command="load" />
    <rosparam file="$(find racebot_gazebo)/config/xt_teb/base_global_planner_params.yaml" command="load" />
    <rosparam file="$(find racebot_gazebo)/config/xt_teb/teb_local_planner_params.yaml" command="load" />
    <remap from="/cmd_vel" to="/ackermann_steering_controller/cmd_vel" />
  </node>


</launch> 